<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="line">
            <stroke android:width="@dimen/st_vod_seek_bar_background_thickness" android:color="#ffffffff"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="line">
                <stroke android:width="@dimen/st_vod_seek_bar_background_thickness" android:color="#ff00e0e4"/>
            </shape>
        </clip>
    </item>
</layer-list>
