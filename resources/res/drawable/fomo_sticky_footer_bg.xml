<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#00000000" android:endColor="#90000000" android:angle="-90"/>
        </shape>
    </item>
    <item android:top="4dp">
        <shape android:shape="rectangle">
            <corners android:topLeftRadius="16dp" android:topRightRadius="16dp"/>
            <stroke android:width="2dp" android:color="@color/res_0x7f0605d5"/>
            <gradient android:startColor="@color/d11_color_bg__highlightLightest" android:endColor="@color/d11_color_bg__neutralLightest" android:angle="-90"/>
        </shape>
    </item>
    <item android:gravity="top|left" android:drawable="@drawable/ic_sticky_drawer_pattern_left" android:left="2dp" android:top="6dp"/>
    <item android:gravity="top|right" android:left="2dp" android:top="6dp" android:right="2dp">
        <selector>
            <item android:drawable="@drawable/ic_sticky_drawer_pattern_right"/>
        </selector>
    </item>
</layer-list>
